citHeader("To cite sonicscrewdriver in publications use:")

citEntry(
  entry    = "Manual",
  title    = "SonicScrewdriveR",
  author   = "Ed Baker",
  year     = "2021",
  url      = "https://cran.r-project.org/package=sonicscrewdriver",
  textVersion = paste(
    "Baker, E. (2021) SonicScrewdriveR. https://cran.r-project.org/package=sonicscrewdriver."
  )
)
